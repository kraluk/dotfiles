Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! material from r/unixporn

urxvt.font:                xft:inconsolata for powerline:size=12
urxvt.boldFont:            xft:inconsolata for powerline:size=12:bold
urxvt.italicFont:          xft:inconsolata for powerline:size=12:italic
urxvt.boldItalicFont:      xft:inconsolata for powerline:size=12:bold:italic
!urxvt.letterSpace:         -1

#define base00 #263238 
#define base01 #2E3D45 
#define base02 #455B66
#define base03 #567280
#define base04 #799FB3
#define base05 #93C2D9
#define base06 #ADE4FF 
#define base07 #FFFFFF 
#define base08 #FF4081 // red
#define base09 #FF7043 // orange
#define base0A #FFA21A // yellow
#define base0B #8BC34A // green
#define base0C #00BCD4 // cyan
#define base0D #2979FF // blue
#define base0E #9C27B0 // purple
#define base0F #7986CB // indigo

*foreground:   base05
*background:   [95]base00
*cursorColor:  #FFCC00

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05
*.color8:       base03
*.color9:       base08
*.color10:      base0B
*.color11:      base0A
*.color12:      base0D
*.color13:      base0E
*.color14:      base0C
*.color15:      base07

urxvt.depth: 32
urxvt.scrollBar: false
urxvt.perl-ext-common: default,matcher,resize-font,keyboard-select,clipboard
urxvt.url-launcher: /usr/bin/xdg-open
urxvt.matcher.button: 1
urxvt.matcher.rend.0: Uline

urxvt.resize-font.smaller: C-minus
urxvt.resize-font.bigger: C-plus
urxvt.resize-font.reset: C-equal
urxvt.resize-font.show: C-question

urxvt.iso14755: false
urxvt.iso14755_52: false

urxvt.keysym.M-Escape: perl:keyboard-select:activate
urxvt.keysym.C-Escape: command: \033[6 q

! rofi configuration
rofi.font:				Cantarell 15
rofi.location:          0
rofi.color-enabled:     true
rofi.color-window:      argb:B3000000, argb:B3000000, argb:E6ffffff
rofi.color-normal:      argb:00000000, argb:90FFFFFF, argb:00000000, argb:00000000, argb:FFFFFFFF 
!ffe1a3ee
rofi.color-urgent:      argb:00282a2e, argb:90FFFFFF, argb:00282a2e, argb:00000000, argb:ffcc6666
rofi.color-active:      argb:00282a2e, argb:90FFFFFF, argb:00282a2e, argb:00000000, argb:ff81a2be
rofi.separator-style:   solid
rofi.fullscreen:        true
rofi.padding:           220
rofi.hide-scrollbar:    true
rofi.line-margin:       4
rofi.yoffset:           0
rofi.xoffset:           0
rofi.width:             100
rofi.lines:             12
